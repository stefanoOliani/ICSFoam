FoamFile
{
    version     2.3;
    format      ascii;
    class       dictionary;
    object      decomposeParDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


numberOfSubdomains  4;

method  scotch;

coeffs
{
}

constraints
{
    patches
    {
        type    preservePatches;
        patches (".*");
        enabled true;
    }
}
